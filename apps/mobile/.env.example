# ==================================
# REQUIRED - Core API Configuration
# ==================================

# Base URL for API calls (MUST be HTTPS for iOS ATS compliance)
EXPO_PUBLIC_BASE_URL=https://your-api-domain.com

# Proxy URL for second-party requests (if using a proxy)
EXPO_PUBLIC_PROXY_BASE_URL=https://your-proxy-domain.com

# Host header for API requests
EXPO_PUBLIC_HOST=your-api-domain.com

# Project group ID for authentication
EXPO_PUBLIC_PROJECT_GROUP_ID=your-project-group-id

# ==================================
# REQUIRED - App Configuration
# ==================================

# App URL for deep linking
EXPO_PUBLIC_APP_URL=https://your-app-url.com

# User content CDN URL (for uploaded files)
EXPO_PUBLIC_BASE_CREATE_USER_CONTENT_URL=https://ucarecdn.com

# ==================================
# OPTIONAL - Third-Party Services
# ==================================

# Uploadcare public key for file uploads
EXPO_PUBLIC_UPLOADCARE_PUBLIC_KEY=your-uploadcare-key

# Google Maps API key for map features
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-key

# ==================================
# OPTIONAL - Error Reporting
# ==================================

# Logging endpoint for error reports
EXPO_PUBLIC_LOGS_ENDPOINT=https://your-logging-service.com

# Temporary API key for error reporting
EXPO_PUBLIC_CREATE_TEMP_API_KEY=your-temp-api-key

# Dev server ID for development debugging
EXPO_PUBLIC_DEV_SERVER_ID=your-dev-server-id

# ==================================
# OPTIONAL - Feature Flags
# ==================================

# Enable "Anything" app mode (set to true in JSON format)
# EXPO_PUBLIC_IS_ANYTHING_APP=true
